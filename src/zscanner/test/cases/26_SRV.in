@	SRV	0	1h	0	@		; Bad weight
@	SRV	0	0	1h	@		; Bad port
@	SRV	0	0	0	%		; Bad target
@	SRV	65536	0	0	@		; Priority overflow
@	SRV	0	65536	0	@		; Weight overflow
@	SRV	0	0	65536	@		; Port overflow
