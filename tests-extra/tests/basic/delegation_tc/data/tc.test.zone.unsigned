; delegation with mixed location of servers
mixed		DS	0 0 100 
mixed		NS	ns1.glue      ; others glue
		NS	ns1.mixed     ; glue
		NS	ns2.mixed     ; unreachable
		NS	ns1.foreign.  ; foreign
		NS	ns1           ; in zone
		NS	mixed         ; self
