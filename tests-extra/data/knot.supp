{
   liburcu/call_rcu
   Memcheck:Leak
   match-leak-kinds: possible,reachable
   fun:calloc
   ...
   fun:call_rcu_memb
}

{
   liburcu/call_rcu
   Memcheck:Leak
   match-leak-kinds: possible,reachable
   fun:malloc
   ...
   fun:call_rcu_memb
}

{
   liburcu/call_rcu
   Memcheck:Leak
   match-leak-kinds: possible,reachable
   fun:malloc
   ...
   fun:urcu_memb_call_rcu
}

{
   pthread/shared_stack
   Memcheck:Leak
   match-leak-kinds: possible,reachable
   fun:calloc
   ...
   fun:allocate_stack
}

{
   libgnutls/p11_load_module
   Memcheck:Leak
   match-leak-kinds: definite,reachable
   fun:*alloc
   ...
   fun:gnutls_pkcs11_add_provider
}

{
   libstdc++/exit
   Memcheck:Leak
   match-leak-kinds: reachable
   fun:malloc
   ...
   fun:exit
}
